{
  "name": "cosourcery",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "local-ssr": "npm run build:ssr && npm run serve:ssr",
    "deploy:ssr": "npm run build:ssr && gcloud app deploy",
    "gcloud-deploy-beta": "gcloud beta app deploy --version dev",
    "gcloud-stop": "gcloud app versions stop dev",
    "gcloud-start": "gcloud app versions start dev",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "emulate": "cd functions && firebase emulators:start --import=./testdb --export-on-exit && cd ..",
    "watch:functions": "npm run watch --prefix functions",
    "import:testdb": "gcloud firestore export gs://cosourcerytest.appspot.com/rawFirestore && cd functions && gsutil -m cp -r gs://cosourcerytest.appspot.com/rawFirestore . && cd ..",
    "import:proddb": "echo we have not set this up yet",
    "compile:server_bak": "tsc -p server.tsconfig.json",
    "build:ssr_bak": "npm run build:client-and-server-bundles && npm run compile:server",
    "serve:ssr_bak": "node dist/server",
    "build:client-and-server-bundles_bak": "ng build --prod && ng run cosourcery:server:production",
    "dev:ssr": "ng run cosourcery:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run cosourcery:server:production",
    "prerender": "ng run cosourcery:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.6",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "~10.1.6",
    "@angular/compiler": "~10.1.6",
    "@angular/core": "~10.1.6",
    "@angular/fire": "^6.0.3",
    "@angular/forms": "~10.1.6",
    "@angular/material": "^10.2.5",
    "@angular/platform-browser": "~10.1.6",
    "@angular/platform-browser-dynamic": "~10.1.6",
    "@angular/platform-server": "~10.1.6",
    "@angular/router": "~10.1.6",
    "@google-cloud/debug-agent": "^4.0.0",
    "@nguniversal/express-engine": "^10.1.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "firebase": "^7.23.0",
    "lodash": "^4.17.20",
    "ngx-quill": "^12.0.1",
    "quill": "^1.3.7",
    "quill-image-resize": "^3.0.9",
    "quill-image-resize-module": "^3.0.0",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.7",
    "@angular/cli": "^10.1.7",
    "@angular/compiler-cli": "~10.1.6",
    "@angular/language-service": "~10.1.6",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.12.41",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
